{
  "name": "StoryPoint for Asana",
  "version": "0.0.13",
  "manifest_version": 2,
  "description": "track your scrum story points on your Asana board.",
  "icons": {
    "16": "images/icon-16.png",
    "128": "images/icon-128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts.babel/chromereload.js",
      "scripts.babel/background.js"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "scripts.babel/contentscript.js"
      ],
      "matches": [
        "https://app.asana.com/*"
      ]
    }
  ],
  "permissions": [
  ]
}
